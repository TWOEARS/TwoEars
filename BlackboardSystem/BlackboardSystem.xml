<?xml version="1.0" encoding="utf-8"?>
<!-- Configure dependency of the Two!Ears Blackboard System module -->

<!-- Start the Two!Ears Blackboard System with
     startTwoEars('BlackboardSystem.xml'); -->

<requirements>
    <TwoEarsPart startup="startBinauralSimulator">binaural-simulator</TwoEarsPart>
    <TwoEarsPart startup="startAuditoryFrontEnd">auditory-front-end</TwoEarsPart>
    <TwoEarsPart startup="startBlackboardSystem">blackboard-system</TwoEarsPart>
    <TwoEarsPart startup="startTools" >tools</TwoEarsPart>
</requirements>
